<h1> New Car: </h1>
<%= form_tag '/cars', method: :post do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name  %>
  </div>
  <div>
    <%= label_tag :price %>
    <%= text_field_tag :price  %>
  </div>
  <div>
    <%= label_tag :image %>
    <%= text_field_tag :image  %>
  </div>
  <div>
    <%= label_tag :description %>
    <%= text_field_tag :description  %>
  </div>
  <div>
    <%= submit_tag "Add car" %>
  </div>
<% end %>
