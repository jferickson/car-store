<h1>All Cars</h1>
<div class="row">
<% @cars.each do |car| %>
<div>
<div class="col-md-4">
<div class="well">
<a href = "/cars/<%= car.id %>"><p><%= car.name %></p></a>
<p><%= number_to_currency(car.price) %></p>
<p><%= car.description %></p>
<img src = <%= car.images[0].url %> height = "300" width = "300">
</div>
</div>
<% end %>
</div>